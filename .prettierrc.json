{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "printWidth": 120,
  "useTabs": true,
  "singleQuote": true,
  "tabWidth": 4,
  "trailingComma": "none",
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "bracketSpacing": true,
        "explicitTypes": "always",
        "tabWidth": 4,
        "singleQuote": false,
        "trailingComma": "none"
      }
    }
  ]
}
